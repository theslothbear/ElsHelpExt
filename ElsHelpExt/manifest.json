{
  "name": "ElsHelp",
  "description": "Extension for ElSchool",
  "version": "1.1",
  "manifest_version": 3,
  "host_permissions": ["*://elschool.ru/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
        
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "notifications",
    "storage",
    "declarativeNetRequest",
    "alarms",
    "tabs"
  ]
}